        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <AssociationSetMapping Name="CartUser" TypeName="EFModel.CartUser" StoreEntitySet="Carts">
      <EndProperty Name="User">
        <ScalarProperty Name="UserId" ColumnName="User_UserId" />
      </EndProperty>
